  "dependencies": {
    "assert": "^1.4.1",
    "axios": "^0.13.1",
    "lodash": "^4.17.4",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.3.0",
    "react-pager": "^1.2.1",
    "react-router": "^2.6.1",
    "unique-id-mixin": "^1.0.0"
