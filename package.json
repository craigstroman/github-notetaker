    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sequelize": "^5.21.12",
    "striptags": "^2.2.1",
    "unique-id-mixin": "^1.0.0"
  },
  "engines": {
    "node": ">=7.2.1"
  }
