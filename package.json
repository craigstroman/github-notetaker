{
  "name": "github-note-taker",
  "version": "2.2.0",
  "description": "A React app that you can browse Github repositories with.",
  "repository": {
    "type": "git",
    "url": "https://github.com/craigstroman/github-notetaker.git"
  },
  "main": "src/js/app.js",
  "scripts": {
    "start": "NODE_ENV=production babel-node ./server/index.js",
    "scss": "node-sass server/views/scss/styles.scss public/css/styles.css -w -o",
    "js": "NODE_ENV=development webpack --config webpack.config.dev.js --progress --watch",
    "copy-files": "copyfiles -f ./client/css/styles.css ./public/css & copyfiles -f ./client/js/libs/*.js ./public/js/libs & copyfiles -f ./client/fonts/bootstrap/*.* ./public/fonts/bootstrap",
    "live:client": "NODE_ENV=development webpack --config webpack.config.dev.js --progress --watch",
    "live:server": "NODE_ENV=development nodemon ./server/index.js --exec babel-node './server/index.js'",
    "prod:build": "NODE_ENV=production webpack --config webpack.config.prod.js & npm run copy-files",
    "prod:server": "NODE_ENV=production nodemon ./server/index.js --exec babel-node './server/index.js'"
  },
  "author": "Craig Stroman",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-console": "^6.9.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-minify": "^0.4.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "cuid": "^1.3.8",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.8.1",
    "nodemon": "^1.12.1",
    "require-dir": "^0.3.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.7",
    "@fortawesome/fontawesome-free-solid": "^5.0.12",
    "@fortawesome/react-fontawesome": "0.0.19",
    "app-module-path": "^2.2.0",
    "assert": "^1.4.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "config": "^1.26.2",
    "cookie-parser": "^1.4.3",
    "cross-fetch": "^2.1.0",
    "express": "^4.15.5",
    "express-session": "^1.15.6",
    "express-session-mongo": "^0.1.0",
    "html-to-text": "^3.3.0",
    "lodash": "^4.17.5",
    "mongoose": "^4.11.13",
    "mongoose-find-or-create": "^1.3.1",
    "morgan": "^1.8.2",
    "npm": "^5.7.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "prop-types": "^15.6.1",
    "pug": "^2.0.0-rc.4",
    "query-string": "^6.0.0",
    "querystring": "^0.2.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "striptags": "^2.2.1",
    "unique-id-mixin": "^1.0.0"
  },
  "engines": {
    "node": ">=7.2.1"
  }
}
