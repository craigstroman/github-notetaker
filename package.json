{
  "name": "github-note-taker",
  "version": "1.5.2",
  "description": "A React app that you can browse Github repositories with.",
  "repository": {
    "type": "git",
    "url": "https://github.com/craigstroman/github-notetaker.git"
  },
  "main": "src/js/app.js",
  "scripts": {
    "scss": "node-sass --output-style nested indent-type tab --indent-width 4 -w -o src/css src/css/scss",
    "js": "NODE_ENV=development webpack --progress --watch",
    "build": "NODE_ENV=production webpack && gulp build -p ",
    "start": "npm run js",
    "dev": "NODE_ENV=development webpack-dev-server",
    "prod": "NODE_ENV=production http-server ./dist"
  },
  "author": "Craig Stroman",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.0.3",
    "gulp-eslint": "^3.0.1",
    "gulp-html-replace": "^1.6.2",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "http-server": "^0.9.0",
    "node-sass": "^4.5.0",
    "require-dir": "^0.3.1",
    "uglify-js-plugin": "0.0.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
